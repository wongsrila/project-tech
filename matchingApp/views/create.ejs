<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs')  %> 
<body>

    <!-- navigation -->
    <%- include('./partials/nav.ejs')  %>

    <!-- Content -->
    <div class="create-blog content">
        <form action="">
            <label for="title">Blog Title:</label>
            <input type="text" id="title" required>
