            <label for="body">Blog body:</label>
            <textarea id="body" cols="30" rows="10" required></textarea>
            <button>Submit</button>
        </form>
    </div>

    <!-- Footer -->
    <%- include('./partials/footer.ejs')  %> 
</body>
</html>